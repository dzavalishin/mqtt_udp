PROGRAM PLC_PRG
VAR

	Need_init: BOOL := TRUE;


	Zinit:		Z033_INIT;
	Reader:	GET_DATA;

	z033Write:	Z033_WRITER;

	Read1W: DZ1W_READER;

	z033_temperature_set_old	: WORD;
	Uplink_Target_Temp_old		: WORD;

	z033_humidity_set_old		: WORD;
	Uplink_Target_Humid_old	: WORD;



END_VAR

(*
VAR RETAIN PERSISTENT
	GLOBAL_Target_Room_Temp: REAL;
END_VAR
*)
(* @END_DECLARATION := '0' *)
IF Need_init THEN


	z033_cmd := 255;
	z033_special_cmd := 255;

	mva8_0_cmd := 255;
	mva8_1_cmd := 255;
	mva8_2_cmd := 255;

	dz1w_cmd := 255;

	Zinit();
	z033Write();

	z033_temperature115_out := REAL_TO_WORD( GLOBAL_Target_Room_Temp );
	z033_hum116_out := REAL_TO_WORD( GLOBAL_Target_Room_Humid );


	Need_init := FALSE;
END_IF

(* TODO check if z033 really needs even parity. reprogram? *)


	Reader();
	Read1W();

	(* temp set *)

	IF (Reader.z033_temperature_set > SANE_TEMP_MIN) AND (Reader.z033_temperature_set < SANE_TEMP_MAX) AND (z033_temperature_set_old <> Reader.z033_temperature_set) THEN
		GLOBAL_Target_Room_Temp := Reader.z033_temperature_set;
		z033_temperature_set_old := Reader.z033_temperature_set;

		z033_temperature115_out := Reader.z033_temperature_set;

		Uplink_Target_Temp := REAL_TO_BYTE( GLOBAL_Target_Room_Temp );
		Uplink_Target_Temp_old := Uplink_Target_Temp;
	END_IF

	IF Uplink_Target_Temp_old <> Uplink_Target_Temp THEN
		GLOBAL_Target_Room_Temp := Uplink_Target_Temp;
		Uplink_Target_Temp_old := Uplink_Target_Temp;
	END_IF

	(* Fix crazy valiues *)

	IF GLOBAL_Target_Room_Temp < SANE_TEMP_MIN THEN
		GLOBAL_Target_Room_Temp := SANE_TEMP_MIN;
	END_IF

	IF GLOBAL_Target_Room_Temp > SANE_TEMP_MAX THEN
		GLOBAL_Target_Room_Temp := SANE_TEMP_MAX;
	END_IF


	(* humidity set *)



	IF (Reader.z033_humidity_set > SANE_HUMID_MIN) AND (Reader.z033_humidity_set < SANE_HUMID_MAX) AND (z033_humidity_set_old <> Reader.z033_humidity_set) THEN
		GLOBAL_Target_Room_Humid := Reader.z033_humidity_set;
		z033_humidity_set_old := Reader.z033_humidity_set;

		z033_hum116_out:= Reader.z033_humidity_set;

		Uplink_Target_Humid := REAL_TO_BYTE( GLOBAL_Target_Room_Humid );
		Uplink_Target_Humid_old := Uplink_Target_Humid;
	END_IF

	IF Uplink_Target_Humid_old <> Uplink_Target_Humid THEN
		GLOBAL_Target_Room_Humid := Uplink_Target_Humid;
		Uplink_Target_Humid_old := Uplink_Target_Humid;
	END_IF

	(* Fix crazy valiues *)

	IF GLOBAL_Target_Room_Humid < SANE_HUMID_MIN THEN
		GLOBAL_Target_Room_Humid := SANE_HUMID_MIN;
	END_IF

	IF GLOBAL_Target_Room_Humid > SANE_HUMID_MAX THEN
		GLOBAL_Target_Room_Humid := SANE_HUMID_MAX;
	END_IF


	(* TODO no net vars now, do it some other way *)
	(* Lower HUMIDITY in bathroom, if too high * )

	IF GLOBAL_Current_Bath_Air_Humid > GLOBAL_Target_Room_Humid THEN

		(* +10% of humidity = 100% of ventilation power *)
		Net_Ventilation_Request_Percent := REAL_TO_INT( MIN( 99, (GLOBAL_Current_Bath_Air_Humid - GLOBAL_Target_Room_Humid) * 10 ) );

	ELSE
		Net_Ventilation_Request_Percent := 0;
	END_IF
	*)





	z033Write();

END_PROGRAM

