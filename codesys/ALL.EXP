		(* 2#1110000000000000 (* hum measure points *) *)
		   2#0100000000000000 (* hum measure points - just ROOM *)
		;

	z033_Setup113 := 16#0714;

	z033_mode114_out := 2#01010001;

	z033_errmode11c_out := 0;

(*
