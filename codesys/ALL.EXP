(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/HTTP' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK HTTP_CLIENT
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR

VAR

	m_dnSocket		:	DINT	:= SOCKET_INVALID;
(*	m_SAddress		:	SOCKADDRESS; *)

	m_error			:	INT		:= 0;

(*	o_dnRes			:	DINT; *)

	state				:	INT		:= 0;

	CONNECT		:	HTTP_CONNECT;

END_VAR

VAR CONSTANT
	wOutPort: INT := 8080;

	yOutAddr1: BYTE := 192;
	yOutAddr2: BYTE := 168;
	yOutAddr3: BYTE := 1;
	yOutAddr4: BYTE := 141;
(*	yOutAddr4: BYTE := 100; *)

(*
	yOutAddr4: BYTE := 192;
	yOutAddr3: BYTE := 168;
	yOutAddr2: BYTE := 1;
	yOutAddr1: BYTE := 100;
*)

	request: STRING := 'GET / HTTP/1.0$R$L$R$L';
END_VAR
(* @END_DECLARATION := '0' *)


	IF m_dnSocket = SOCKET_INVALID THEN

		CONNECT(  yOutAddr1:= yOutAddr1, yOutAddr2 := yOutAddr2, yOutAddr3 := yOutAddr3, yOutAddr4 := yOutAddr4, wOutPort := wOutPort, m_dnSocket := m_dnSocket, m_error := m_error  );
		RETURN; (* this shit needs some timeout between connect and send! *)

	END_IF


	CASE state  OF

	0:	HTTP_SEND_REAL( m_dnSocket, 'Current_Children_Floor_Temp', GLOBAL_Current_Children_Floor_Temp );
	1:	HTTP_SEND_REAL( m_dnSocket, 'Current_Children_Air_Temp', GLOBAL_Current_Children_Air_Temp );

	2:	HTTP_SEND_REAL( m_dnSocket, 'Current_Dinner_Floor_Temp', GLOBAL_Current_Dinner_Floor_Temp );
	3:	HTTP_SEND_REAL( m_dnSocket, 'Current_Dinner_Air_Temp', GLOBAL_Current_Dinner_Air_Temp );

	4:	HTTP_SEND_REAL( m_dnSocket, 'Current_Bath_Air_Temp', GLOBAL_Current_Bath_Air_Temp );
	5:	HTTP_SEND_REAL( m_dnSocket, 'Current_Bath_Floor_Temp', GLOBAL_Current_Bath_Floor_Temp );

	6:	HTTP_SEND_REAL( m_dnSocket, 'Current_Entrance_Air_Temp', GLOBAL_Current_Entrance_Air_Temp );
	7:	HTTP_SEND_REAL( m_dnSocket, 'Current_Entrance_Floor_Temp', GLOBAL_Current_Entrance_Floor_Temp );

	8:	HTTP_SEND_REAL( m_dnSocket, 'Current_Bath_Tube_Temp', GLOBAL_Current_Bath_Tube_Temp );

	9:	HTTP_SEND_REAL( m_dnSocket, 	'Current_Bath_Air_Humid', GLOBAL_Current_Bath_Air_Humid );

	10:	HTTP_SEND_REAL( m_dnSocket, 	'Current_Outdoor_Air_Temp', GLOBAL_Current_Outdoor_Air_Temp );

	11:	HTTP_SEND_REAL( m_dnSocket, 	'Current_Dinner_Air_Bottom_Temp', GLOBAL_Current_Dinner_Air_Bottom_Temp );
	12:	HTTP_SEND_REAL( m_dnSocket, 	'Current_Dinner_Air_Middle_Temp', GLOBAL_Current_Dinner_Air_Middle_Temp );

	13:	HTTP_SEND_REAL( m_dnSocket, 	'Current_Kitchen_Air_Ceiling_Temp', GLOBAL_Current_Kitchen_Air_Ceiling_Temp );

	14:	HTTP_SEND_REAL( m_dnSocket, 	'Current_Badroom_Air_Temp', GLOBAL_Current_Badroom_Air_Temp );

	ELSE
		state := -1;

		HTTP_SEND_REAL( m_dnSocket, 'Target_Room_Temp', GLOBAL_Target_Room_Temp );


	END_CASE

	state := state + 1;

	SysSockClose ( m_dnSocket );
	m_dnSocket := SOCKET_INVALID;


	m_error := 0;



END_FUNCTION_BLOCK


