FUNCTION MIN_SANE_TEMP : REAL

VAR_INPUT
	Ta, Tb : REAL;
END_VAR

VAR
	Ta_SANE, Tb_SANE : BOOL;
END_VAR
(* @END_DECLARATION := '0' *)

Ta_SANE := IS_SANE_TEMP( Ta );
Tb_SANE := IS_SANE_TEMP( Tb );

IF Ta_SANE AND NOT Tb_SANE THEN
	MIN_SANE_TEMP := Ta;
	RETURN;
END_IF;

IF Tb_SANE AND NOT Ta_SANE THEN
	MIN_SANE_TEMP := Tb;
	RETURN;
END_IF;

IF Ta_SANE AND Tb_SANE THEN
	MIN_SANE_TEMP := MIN( Ta, Tb );
	RETURN;
END_IF;


	MIN_SANE_TEMP := SANE_TEMP_MIN-100; (* Surely insane low value *)

END_FUNCTION


