(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/IO' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK GET_DATA

VAR_INPUT
END_VAR

VAR_OUTPUT
	z033_temperature_set: WORD;
	z033_humidity_set: WORD;
	z033_room_temp: WORD;
	z033_mode: WORD;
	z033_vent_speed: WORD;
	z033_vent_auto: BOOL;
	z033_mode_vent: BOOL;
	z033_mode_heat: BOOL;
	z033_mode_cool: BOOL;
	z033_mode_humid: BOOL;
	z033_mode_dry: BOOL;
	mva_temperature: ARRAY[0..7] OF REAL;
END_VAR

VAR
     TEMP_REAL : REAL;
END_VAR
(* @END_DECLARATION := '0' *)


(* Gather all the data we need as input *)


(* Read from Z033 - check 'changed' bit first *)

IF (z033_errmode11c_in AND 16#1000) <> 0 THEN

	z033_errmode11c_out := 0; (* reset 'changed' bit and error state - TODO if we will use error state - handle it here too *)

	(* TODO read if we are initing *)


END_IF

	(* TODO read if changed, see above *)


	z033_mode := z033_mode114_in;
	z033_temperature_set := z033_temperature115_in; (* z033_temperature115_out := z033_temperature115_in; *)
	z033_humidity_set := z033_hum116_in;
	z033_room_temp	:= z033_room_temp117_in;

	z033_vent_speed := SHR( z033_mode, 1 ) AND 2#111;
	z033_vent_auto := (SHR( z033_mode, 3 ) AND 2#1) <> 0;

	z033_mode_vent := (SHR( z033_mode, 4 ) AND 2#1) <> 0;
	z033_mode_heat := (SHR( z033_mode, 5 ) AND 2#1) <> 0;
	z033_mode_cool := (SHR( z033_mode, 6 ) AND 2#1) <> 0;
	z033_mode_humid := (SHR( z033_mode, 7 ) AND 2#1) <> 0;
	z033_mode_dry := (SHR( z033_mode, 8 ) AND 2#1) <> 0;

	(* GLOBAL_Current_Dinner_Air_Temp := MIN_SANE_TEMP( z033_room_temp / 10.0, MVA0_t5 ) ; *)
	(* 0t5 = kitchen ceiling sensor, 1t5 = near floor down the window dinner room sensor *)

	(* GLOBAL_Current_Dinner_Air_Temp := MIN_SANE_TEMP( z033_room_temp / 10.0, MIN_SANE_TEMP( MVA0_t5, mva1_t3 ) ) ; *)
	GLOBAL_Current_Dinner_Air_Temp := AVG_SANE_TEMP3( z033_room_temp / 10.0, MVA0_t5, mva1_t3 );

	GLOBAL_Current_Dinner_Air_Bottom_Temp	:=	mva1_t3;
	GLOBAL_Current_Dinner_Air_Middle_Temp	:=	z033_room_temp / 10.0;
	GLOBAL_Current_Kitchen_Air_Ceiling_Temp	:=	MVA0_t5;
	GLOBAL_Current_Guest_Air_Temp := AVG_SANE_TEMP( mva2_t0, mva2_t1 ) ; (* guest room floor and ceiling (fire sensor) *)

	(* GLOBAL_Target_Room_Temp := z033_temperature_set; *)

	mva_temperature[0] := mva0_t0; (* 1/2 children floor *)
	mva_temperature[1] := mva0_t1; (* 1/2 children floor *)
	mva_temperature[2] := mva0_t2;	(* dinner floor *)
	mva_temperature[3] := mva0_t3;	(* entrance floor *)
	mva_temperature[4] := mva0_t4;	(* bath floor *)
	mva_temperature[5] := mva0_t5; (* Kitchen ceiling *)
	mva_temperature[6] := mva0_t6;
	mva_temperature[7] := mva0_t7; (* server room (equipment closet) top *)



	GLOBAL_Current_Children_Floor_Temp	:= MAX( mva0_t0, mva0_t1 );
	GLOBAL_Current_Dinner_Floor_Temp		:= mva0_t2;
	GLOBAL_Current_Entrance_Floor_Temp	:= mva0_t3;
	GLOBAL_Current_Bath_Floor_Temp		:= mva0_t4;

	GLOBAL_Current_Entrance_Air_Temp		:= MIN_SANE_TEMP( mva0_t7, mva0_t6 ); (* mva0_t7; (* currently in equipment closet *) *)

	GLOBAL_Current_Children_Air_Temp		:= MIN_SANE_TEMP( mva1_t0, mva1_t1 );

	GLOBAL_Current_Badroom_Air_Temp		:= mva1_t2;

	GLOBAL_Current_Bath_Air_Temp			:= 	mva1_t6;
	GLOBAL_Current_Bath_Tube_Temp		:= 	mva1_t7;

	TEMP_REAL	:= HUMID_CONV( V_In := mva1_t4 * 4 / 100, Zero_Off := 0.842, Slope := 0.031 );

	(* Sometimes we get insane value *)
	(* Assign sane data only *)
	IF (TEMP_REAL > 5) AND (TEMP_REAL < 95) THEN
		GLOBAL_Current_Bath_Air_Humid			:= LIMIT_VALUE( TEMP_REAL, 0, 100 );
	END_IF

	GLOBAL_Current_Outdoor_Air_Temp		:= mva1_t5;

END_FUNCTION_BLOCK


