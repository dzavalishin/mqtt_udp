(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/IO' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK Z033_WRITER

VAR_INPUT
	Enforce : BOOL := FALSE;

	z033_vent_speed: WORD;
	z033_vent_auto: BOOL := TRUE;
	z033_mode_vent: BOOL;
	z033_mode_heat: BOOL := TRUE;
	z033_mode_cool: BOOL;
	z033_mode_humid: BOOL;
	z033_mode_dry: BOOL;



	(* z033_floor_temp: REAL; *)
	z033_outdoor_hum: REAL;
	z033_room_hum: REAL;
	z033_air_hum: REAL;
END_VAR

VAR_OUTPUT
END_VAR

VAR
	z033_mode: WORD;
	z033_mode_old: WORD;
	TEMP: WORD;
	z033_outdoor_hum119_out_old: WORD;
	z033_room_hum11a_out_old: WORD;
	z033_air_hum11b_out_old: WORD;

	z033_floor_temp118_out_old: REAL;
	z033_air_temp_117_out_old: REAL;

	GLOBAL_Target_Room_Temp_old: WORD;

	GLOBAL_Target_Room_Humid_old : WORD;
	GLOBAL_Current_Bath_Air_Humid_old: WORD;

	(* Set actual date and time *)

	(* z033_DateTime, Minutes, Hours, WeekDay: WORD; *)

	z033_DateTime				: WORD;
	systime						: SysTime64;
	now							: SystemTimeDate;
	TimeReader					: CurTimeEx;

END_VAR
(* @END_DECLARATION := '0' *)
(* Write current values to Z033 panel *)

	z033_mode := 1; (* lower bit - panel is on *)

	z033_mode := z033_mode OR SHL(z033_vent_speed AND 2#111, 1);

	IF z033_vent_auto THEN
		z033_mode := z033_mode OR 2#10000;
	END_IF

	IF z033_mode_vent THEN
		z033_mode := z033_mode OR 2#100000;
	END_IF

	IF z033_mode_heat THEN
		z033_mode := z033_mode OR 2#1000000;
	END_IF

	IF z033_mode_cool THEN
		z033_mode := z033_mode OR 2#10000000;
	END_IF

	IF z033_mode_humid THEN
		z033_mode := z033_mode OR 2#100000000;
	END_IF

	IF z033_mode_dry THEN
		z033_mode := z033_mode OR 2#1000000000;
	END_IF


	IF z033_mode <> z033_mode_old THEN
		z033_mode114_out := z033_mode;
		z033_mode_old := z033_mode;
	END_IF

(*
	TEMP := REAL_TO_WORD( z033_outdoor_hum * 10 );

	IF TEMP <> z033_outdoor_hum119_out_old THEN
		z033_outdoor_hum119_out := TEMP;
		z033_outdoor_hum119_out_old := TEMP;
	END_IF


	TEMP := REAL_TO_WORD( z033_room_hum * 10 );

	IF TEMP <> z033_room_hum11a_out_old THEN
		z033_room_hum11a_out := TEMP;
		z033_room_hum11a_out_old := TEMP;
	END_IF



	TEMP := REAL_TO_WORD( z033_air_hum * 10 );

	IF TEMP <> z033_air_hum11b_out_old THEN
		z033_air_hum11b_out := TEMP;
		z033_air_hum11b_out_old := TEMP;
	END_IF
*)

	IF z033_floor_temp118_out_old <> GLOBAL_Current_Dinner_Floor_Temp THEN
		z033_floor_temp118_out := REAL_TO_WORD( GLOBAL_Current_Dinner_Floor_Temp * 10 );
		z033_floor_temp118_out_old := GLOBAL_Current_Dinner_Floor_Temp;
	END_IF

	(* temp off - wrong
	IF z033_temperature115_out_old <> GLOBAL_Current_Dinner_Air_Temp THEN
		z033_temperature115_out := REAL_TO_WORD( GLOBAL_Current_Dinner_Air_Temp * 10 );
		z033_temperature115_out_old := GLOBAL_Current_Dinner_Air_Temp;
	END_IF
	*)

	(* temp replacement *)
	IF z033_air_temp_117_out_old <> GLOBAL_Current_Entrance_Air_Temp THEN
		z033_air_temp_117_out := REAL_TO_WORD( GLOBAL_Current_Entrance_Air_Temp * 10 );
		z033_air_temp_117_out_old := GLOBAL_Current_Entrance_Air_Temp;
	END_IF





	TEMP := REAL_TO_WORD( GLOBAL_Target_Room_Temp  );

	IF TEMP <> GLOBAL_Target_Room_Temp_old THEN
		z033_temperature115_out := TEMP;
		GLOBAL_Target_Room_Temp_old := TEMP;
	END_IF


	(* Humidity *)

	TEMP := REAL_TO_WORD( GLOBAL_Target_Room_Humid  );

	(* hum target *)
	IF TEMP <> GLOBAL_Target_Room_Humid_old THEN
		z033_hum116_out := TEMP;
		GLOBAL_Target_Room_Humid_old := TEMP;
	END_IF



	TEMP := REAL_TO_WORD( GLOBAL_Current_Bath_Air_Humid  );

	IF TEMP <> GLOBAL_Current_Bath_Air_Humid_old THEN
		z033_room_hum11a_out := TEMP * 10;
		GLOBAL_Current_Bath_Air_Humid_old := TEMP;
	END_IF


	(* Set actual date and time *)

	now.Day :=0;
	now.DayOfWeek :=0;
	now.dwHighMsec :=0;
	now.dwLowMSecs :=0;
	now.Milliseconds :=0;
	now.Minute :=0;
	now.Second :=0;
	now.Hour :=0;
	now.Year :=0;
	now.Month :=0;

	systime.ulHigh :=0;
	systime.ulLow :=0;

	TimeReader( SystemTime := systime, TimeDate:= now );


	z033_DateTime := 0;

	z033_DateTime := 	UINT_TO_WORD(now.Hour) AND 2#011111;


(*
	z033_DateTime := 	z033_DateTime OR ( SHL( UINT_TO_WORD(now.Minute) ,5) AND 2#011111100000 );
	z033_DateTime := 	z033_DateTime OR ( SHL( UINT_TO_WORD(now.DayOfWeek-1),11) AND 2#011100000000000 );
*)
	z033_DateTime := 	z033_DateTime OR ( SHL( 2#0111111 AND UINT_TO_WORD(now.Minute) ,5)  );
	z033_DateTime := 	z033_DateTime OR ( SHL( 2#0000111 AND UINT_TO_WORD(now.DayOfWeek-1),11)  );



	z033_clock11d_out := z033_DateTime;


END_FUNCTION_BLOCK


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/MQTT_UDP' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK MAKE_MQTT_PKT
VAR_INPUT
	topic : STRING;
	data : STRING;
END_VAR

VAR_OUTPUT
	buf :	STRING;
	len : 	DINT;
END_VAR

VAR
(*	blen :	DINT; *)
	tlen :	DINT;
	dlen :	DINT;
	total : 	DINT;
END_VAR
(* @END_DECLARATION := '0' *)

	tlen := LEN(topic);
	dlen :=LEN(data);

	INSERT( buf, '$30', 0 ); (* 'publish' pkt type *)

	total := tlen + dlen + 2; (* packet size *)

	tlen := SHR( tlen, 8 ) ;

	buf := CONCAT( buf, CHR_TO_STRING( DINT_TO_BYTE( SHR( tlen, 8 ) ) )  );
	buf := CONCAT( buf, CHR_TO_STRING( DINT_TO_BYTE( tlen ) ) );

	buf := CONCAT( buf, topic );
	buf := CONCAT( buf, data );

	(* MAKE_MQTT_PKT := buf; *)

	LEN := total + 2; (* ERROR - supposed that pkt len < 128 and len is one byte *)

END_FUNCTION_BLOCK


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/MQTT_UDP' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
PROGRAM MQTT_MAIN

VAR

	SocketHandle: 	DINT	:= SOCKET_INVALID; (* descriptor of open Socket *)
	addr:				SOCKADDRESS;
	flags:				DINT := 0;
	(*buf:				STRING;
	pkt_size:			DINT := 0;*)

	pkt :				MAKE_MQTT_PKT;

END_VAR
(* @END_DECLARATION := '0' *)

RETURN; (* turn off *)

IF SocketHandle <> SOCKET_INVALID THEN
	SocketHandle := SysSockCreate( SOCKET_AF_INET, SOCKET_DGRAM, SOCKET_IPPROTO_UDP );
END_IF

IF SocketHandle <> SOCKET_INVALID THEN
	RETURN;
END_IF

addr.sin_family := SOCKET_AF_INET;
addr.sin_port :=1883; (* MQTT UDP *)
addr.sin_addr := SOCKET_INADDR_BROADCAST;

pkt( topic := 'plc_topic', data := 'plc_val' );

SysSockSendTo( SocketHandle, ADR(pkt.buf), pkt.LEN, flags, ADR(addr),   SIZEOF(addr)  );

END_PROGRAM


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/MQTT_UDP' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION PACK_LEN : STRING
VAR_INPUT
	data_len : DINT;
END_VAR
VAR
	out : STRING := '';
	b : BYTE;
END_VAR
(* @END_DECLARATION := '0' *)
WHILE TRUE DO


        B := DINT_TO_BYTE( data_len MOD 128 );
        data_len := data_len / 128;

        IF data_len > 0 THEN
            B := B OR DINT_TO_BYTE( 16#80 );
	END_IF

        out := CONCAT( out, CHR_TO_STRING(b) );

        IF( data_len = 0 ) THEN
		EXIT;
	END_IF
END_WHILE

PACK_LEN := out;


END_FUNCTION


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/PID-PWM' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK DUAL_PID

VAR_INPUT
	Target			: REAL;		(* Уставка, target ROOM temperature value to achieve *)
	Current		: REAL;		(* Current measured ROOM temperature value *)

	Floor			: REAL;		(* Current measured FLOOR temperature value *)
	Limited		: REAL := 33;	(* Maximum FLOOR temperature value not to overcome*)


END_VAR

VAR_OUTPUT

	FLOOR_DUTY				: INT;		(* Duty cycle for floor heater *)
	RADIATOR_DUTY				: INT;		(* Duty cycle for wall radiator *)

END_VAR

VAR

	LINEAR			: INT;
	INTEGRAL			: REAL;		(* +- 1000 *)
	DIFFERENTIAL 	: REAL;		(* +- 100 *)
	DUTY				: INT;		(* Duty cycle - total *)

	Diff_Timer			: INT;
	Diff_Prev_Temp	: REAL		:= -1000;
	Diff_Temp_Delta	: REAL;		(* Difference of temperature in one minute *)
	Diff_Sane 			: BOOL;

END_VAR


VAR CONSTANT

	ONLY_RADIATOR_DUTY_LIMIT	: INT := 20;

	Diff_Timer_Time					: INT := 120;

END_VAR

(* @END_DECLARATION := '0' *)

(*

	Actually we have TWO regulators here, one is PID (or PI) - for room temperature,
	and one is simple limiting for floor temperature.

*)

	(* room temp seems to be insane, use floor temp instead *)
	IF Current < 1 THEN
		Current := Floor;
	END_IF




	LINEAR := REAL_TO_INT(  (Target - Current) * 100 / 4 ); (* 4 degrees of difference mean 100% of power *)
	LINEAR := REAL_TO_INT( LIMIT_VALUE( LINEAR, -100, 100) );

	(* Integral part *)

	INTEGRAL := INTEGRAL + (LINEAR / 2500.0); (* integration time about 0,65 hr *)

	INTEGRAL := LIMIT_VALUE( INTEGRAL, -100, 100 );

	(* Diff part *)

	Diff_Sane := TRUE;

	Diff_Timer := Diff_Timer + 1;
	IF Diff_Timer >= Diff_Timer_Time THEN
		Diff_Timer := 0;

		IF NOT IS_SANE_TEMP( Diff_Prev_Temp ) THEN
			Diff_Sane := FALSE;
		END_IF

		Diff_Temp_Delta := Current - Diff_Prev_Temp;
		Diff_Prev_Temp := Current;

	END_IF

	IF NOT Diff_Sane THEN
		Diff_Temp_Delta := 0;
	END_IF

	(* MINUS 0.5 degree per 2 minutes is 400% of power *)
	DIFFERENTIAL := - Diff_Temp_Delta * 200 * 4;

	(* Limit diff part to 60% *)
	DIFFERENTIAL := LIMIT_VALUE( DIFFERENTIAL, 0, 60 );

	(* Common total integral + linear *)

	DUTY := LINEAR +  REAL_TO_INT (INTEGRAL/2); (* max integral part is 50% *)

	(* Just up - we're heating only *)

	DUTY := INT_LIMIT_VALUE( DUTY, 0, 100 );


	(* Radiator has 100% of integral part so that when we have major warmth loss we compensate it mostly with radiator *)
	RADIATOR_DUTY := DUTY + REAL_TO_INT (INTEGRAL/2);


	IF DUTY < ONLY_RADIATOR_DUTY_LIMIT THEN
		FLOOR_DUTY := 0;
	ELSE
		FLOOR_DUTY := (( DUTY- ONLY_RADIATOR_DUTY_LIMIT) * 100) / (100 - ONLY_RADIATOR_DUTY_LIMIT);
	END_IF


(*
	FLOOR_DUTY := DUTY;
	RADIATOR_DUTY := DUTY;
*)

	(* Add differential part just to floor power *)
	FLOOR_DUTY := FLOOR_DUTY + REAL_TO_INT(  DIFFERENTIAL );


	FLOOR_DUTY := INT_LIMIT_VALUE( FLOOR_DUTY, 0, 100 );
	RADIATOR_DUTY := INT_LIMIT_VALUE( RADIATOR_DUTY, 0, 100 );


	(* Simple FLOOR temp limit *)

	IF Floor > Limited THEN
		FLOOR_DUTY := 0;
	END_IF


END_FUNCTION_BLOCK


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/PID-PWM' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK FLOOR_PID

VAR_INPUT
	Target			: REAL;		(* Уставка, target ROOM temperature value to achieve *)
	Current		: REAL;		(* Current measured ROOM temperature value *)

	Floor			: REAL;		(* Current measured FLOOR temperature value *)
	Limited		: REAL := 35;	(* maximum FLOOR temperature value not to overcome*)


END_VAR

VAR_OUTPUT

	DUTY				: INT;		(* Duty cycle for heater *)

END_VAR

VAR

	TMP				: INT;
(*
	INTEGRAL			: INT;
*)
END_VAR


(* @END_DECLARATION := '0' *)

(*

	Actually we have TWO regulators here, one is PID (or PI) - for room temperature,
	and one is simple limiting for floor temperature.

*)

	(* room temp seems to be insane, use floor temp instead *)
	IF Current < 1 THEN
		Current := Floor;
	END_IF


	(* Simple FLOOR temp limit *)

	IF Floor > Limited THEN
		DUTY := 0;
		RETURN;
	END_IF

	(* TEMPORARY IMPL! *)

	IF Current > Target THEN
		DUTY := 0;
		RETURN;
	END_IF

	TMP := REAL_TO_INT(  (Target - Current) * 100 / 4 ); (* 4 degrees of difference mean 100% of power *)

	IF TMP < 0 THEN
		TMP := 0;
	END_IF

	IF TMP > 100 THEN
		TMP := 100;
	END_IF

	DUTY := TMP;


END_FUNCTION_BLOCK


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/PID-PWM' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK PWM

VAR_INPUT
	DUTY 			: UINT;				(* 0-99, percent *)
	HI_VOLTAGE	: BOOL := FALSE; (* By default assume the heater is connected all the time *)
(*
	CYCLE_TIME	: TIME := t#10s;
*)

	PHASE_SHIFT	:	INT := 0;

END_VAR

VAR_OUTPUT
	OUT_ON				: BOOL := FALSE;
	LOAD_DETECTED	: BOOL := FALSE;
END_VAR

VAR
	On_Count				: INT := 0;

	Call_Count 			: INT := 0;
	Check_Load_Count	: INT := 0;	(* Once a while we do check for load to be connected *)

	Blinker					: BLINK;
	Blink_Once			: BOOL := FALSE;
END_VAR

VAR CONSTANT
	Check_Load_Count_Limit	: INT := 128;
	Cycle_Steps					: INT := 10;
END_VAR
(* @END_DECLARATION := '0' *)

(*

We assume that load (actual heater) can be disconnected with a wall switch.
We detect such a situation by turning power off and measuring voltage on heater.
(There must be a resistor over a relay contacts to provide some voltage)

HI_VOLTAGE is just for that.

*)

	(* Once a second *)

(* Called once a second anyway
	Blinker( TimeLow := t#0.5s, TimeHigh := t#0.5s, Enable := TRUE );

	IF NOT Blinker.OUT THEN
		Blink_Once := FALSE;
		RETURN;
	END_IF

	IF Blink_Once THEN
		RETURN;
	END_IF

	Blink_Once := TRUE;
*)
	(* Ok, next second came, do some task *)

	Call_Count := Call_Count + 1;  (* Count seconds *)

	IF NOT OUT_ON THEN (* Check for load *)
		Check_Load_Count := 0; (* No need to turn off - already off *)
		LOAD_DETECTED := NOT HI_VOLTAGE;
	END_IF

	Check_Load_Count := Check_Load_Count + 1;

	IF Check_Load_Count > Check_Load_Count_Limit THEN
		Check_Load_Count := 0;
		(* Override output to be off so that we will check if load is connected on next iteration *)
		OUT_ON := FALSE;
		RETURN;
	END_IF


	IF Call_Count > (Cycle_Steps-1 + PHASE_SHIFT) THEN
		Call_Count := 0 + PHASE_SHIFT;
		On_Count := (DUTY * Cycle_Steps) / 100;
	END_IF

	IF On_Count > 0 THEN
		OUT_ON := TRUE;
		On_Count := On_Count - 1;
	ELSE
		OUT_ON := FALSE;
	END_IF



END_FUNCTION_BLOCK


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/PID-PWM' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION_BLOCK RADIATOR_PWM

(**
 *		Thermal radiator actuator PWM drive.	Works with 24v SR actuator, normally open.
 *
 *		It seems that actuator really works within 0-20% of duty cycle, which means 80-100% for us.
**)


VAR_INPUT
	DUTY 				:	UINT;				(* 0-99, percent *)
	PHASE_SHIFT	:	INT		:= 0;
	ACTIVE			:	BOOL	:= TRUE;

END_VAR

VAR_OUTPUT
	OUT_ON				: BOOL := FALSE;
END_VAR

VAR
	On_Count				: INT := 0;
	Call_Count 			: INT := 0;

	Real_Duty				: INT := 0;

	Blinker					: BLINK;
	Blink_Once			: BOOL := FALSE;
END_VAR

VAR CONSTANT
	Cycle_Steps					: INT := 500; (* we need 500 sec cycle for radiator actuator *)
END_VAR
(* @END_DECLARATION := '0' *)
	(* Must be called once a second *)
	Call_Count := Call_Count + 1;  (* Count seconds *)

	(*Real_Duty := REAL_TO_INT( LIMIT_VALUE( (DUTY / 2) + 50, 0, 100 ) ); *)
	Real_Duty := REAL_TO_INT( DUTY );

	IF Call_Count > (Cycle_Steps-1 + PHASE_SHIFT) THEN
		Call_Count := 0 + PHASE_SHIFT;
		On_Count := (Real_Duty * Cycle_Steps) / 100;
	END_IF

	IF On_Count > 0 THEN
		OUT_ON := TRUE;
		On_Count := On_Count - 1;
	ELSE
		OUT_ON := FALSE;
	END_IF



END_FUNCTION_BLOCK


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
PROGRAM ONCE_A_SECOND_PRG

VAR

	FLOOR_ENTRANCE		: FLOOR_PID;
	FLOOR_BATH				: FLOOR_PID;
	(*FLOOR_DINNER			: FLOOR_PID;*)
	DINNER_PID			: DUAL_PID;
	(*FLOOR_CHILDREN		: FLOOR_PID;*)
	CHILDREN_PID			: DUAL_PID;
	GUEST_PID			: DUAL_PID;

	PWM_ENTRANCE		: PWM;
	PWM_BATH				: PWM;
	PWM_DINNER			: PWM;
	PWM_CHILDREN			: PWM;

(*	PWM_DINNER_RADIATOR		: PWM; *)
	PWM_DINNER_RADIATOR		: RADIATOR_PWM;
	PWM_GUEST_RADIATOR		: RADIATOR_PWM;
	PWM_CHILDREN_RADIATOR	: RADIATOR_PWM;


END_VAR
(* @END_DECLARATION := '0' *)


	(* Must be called once a second! *)



	(* Entrance *)

	FLOOR_ENTRANCE( Target := GLOBAL_Target_Room_Temp, Current := GLOBAL_Current_Entrance_Air_Temp, Floor := GLOBAL_Current_Entrance_Floor_Temp );

	PWM_ENTRANCE( DUTY := FLOOR_ENTRANCE.DUTY, PHASE_SHIFT := 0 );

	HEAT_ENTRANCE_OUT := PWM_ENTRANCE.OUT_ON;



	(* Bath *)

	FLOOR_BATH( Target := GLOBAL_Target_Room_Temp, Current := GLOBAL_Current_Bath_Air_Temp, Floor := GLOBAL_Current_Bath_Floor_Temp );

	PWM_BATH( DUTY := FLOOR_BATH.DUTY, PHASE_SHIFT := 2 );

	HEAT_BATH_OUT := PWM_BATH.OUT_ON;


	(* Dinner *)

(*
	FLOOR_DINNER( Target := GLOBAL_Target_Room_Temp, Current := GLOBAL_Current_Dinner_Air_Temp, Floor := GLOBAL_Current_Dinner_Floor_Temp );
*)
	DINNER_PID( Target := GLOBAL_Target_Room_Temp, Current := GLOBAL_Current_Dinner_Air_Temp, Floor := GLOBAL_Current_Dinner_Floor_Temp );

	PWM_DINNER( DUTY := DINNER_PID.FLOOR_DUTY, PHASE_SHIFT := 4 );

	PWM_DINNER_RADIATOR( DUTY := DINNER_PID.RADIATOR_DUTY, PHASE_SHIFT := 0 );

	HEAT_DINNER_OUT := PWM_DINNER.OUT_ON;

	RADIATOR_DINNER_OUT := NOT PWM_DINNER_RADIATOR.OUT_ON; (* Naive impl - just control radiator just like floor *)

	(* Guest - no hot floor *)

	GUEST_PID( Target := GLOBAL_Target_Room_Temp, Current := GLOBAL_Current_Guest_Air_Temp, Floor := 50 ); (* no floor, give insane input value *)
	PWM_GUEST_RADIATOR( DUTY := GUEST_PID.RADIATOR_DUTY, PHASE_SHIFT := 0 );
	RADIATOR_GUEST_OUT := NOT PWM_GUEST_RADIATOR.OUT_ON; (* Naive impl - just control radiator just like floor *)

	(* Children *)

	(* FLOOR_CHILDREN( Target := GLOBAL_Target_Room_Temp, Current := GLOBAL_Current_CHILDREN_Air_Temp, Floor := GLOBAL_Current_CHILDREN_Floor_Temp ); *)

	CHILDREN_PID(        Target := GLOBAL_Target_Room_Temp, Current := GLOBAL_Current_CHILDREN_Air_Temp, Floor := GLOBAL_Current_CHILDREN_Floor_Temp );

	(*PWM_CHILDREN( DUTY := FLOOR_CHILDREN.DUTY, PHASE_SHIFT := 6 );*)
	PWM_CHILDREN( DUTY := CHILDREN_PID.FLOOR_DUTY, PHASE_SHIFT := 6 );


	PWM_CHILDREN_RADIATOR( DUTY := CHILDREN_PID.RADIATOR_DUTY, PHASE_SHIFT := 0 );

	HEAT_CHILDREN_OUT := PWM_CHILDREN.OUT_ON;

	RADIATOR_CHILDREN_NEAR_OUT := NOT PWM_CHILDREN_RADIATOR.OUT_ON;





END_PROGRAM


(* @NESTEDCOMMENTS := 'Yes' *)
