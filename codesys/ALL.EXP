

(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/Conv' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION AVG_SANE_TEMP : REAL

VAR_INPUT
	Ta, Tb : REAL;
END_VAR

VAR
	Ta_SANE, Tb_SANE : BOOL;
END_VAR
(* @END_DECLARATION := '0' *)

Ta_SANE := IS_SANE_TEMP( Ta );
Tb_SANE := IS_SANE_TEMP( Tb );

IF Ta_SANE AND NOT Tb_SANE THEN
	AVG_SANE_TEMP := Ta;
	RETURN;
END_IF;

IF Tb_SANE AND NOT Ta_SANE THEN
	AVG_SANE_TEMP := Tb;
	RETURN;
END_IF;

IF Ta_SANE AND Tb_SANE THEN
	AVG_SANE_TEMP :=  (Ta + Tb) / 2;
	RETURN;
END_IF;


	AVG_SANE_TEMP := SANE_TEMP_MIN-100; (* Surely insane low value *)

END_FUNCTION


(* @NESTEDCOMMENTS := 'Yes' *)
(* @PATH := '\/Conv' *)
(* @OBJECTFLAGS := '0, 8' *)
(* @SYMFILEFLAGS := '2048' *)
FUNCTION AVG_SANE_TEMP3 : REAL

VAR_INPUT
	Ta, Tb, Tc : REAL;
END_VAR

VAR
	Ta_SANE, Tb_SANE, Tc_SANE : BOOL;
	Sum	: REAL;
	Count	: INT;
END_VAR

(* @END_DECLARATION := '0' *)

Ta_SANE := IS_SANE_TEMP( Ta );
Tb_SANE := IS_SANE_TEMP( Tb );
Tc_SANE := IS_SANE_TEMP( Tb );

Sum := 0;
Count :=0;

IF Ta_SANE THEN
	Sum := Sum + Ta;
	Count := Count + 1;
END_IF;

IF Tb_SANE THEN
	Sum := Sum + Tb;
	Count := Count + 1;
END_IF;

IF Tc_SANE THEN
	Sum := Sum + Tc;
	Count := Count + 1;
END_IF;

IF Count <= 0 THEN
	AVG_SANE_TEMP3 := -100; (* Surely insane low value *)
	RETURN;
END_IF

AVG_SANE_TEMP3 := Sum / Count;

END_FUNCTION


